ConstructorWithParams {

    ConstructorWithParams(int x) {
        print "Hey I am constructor with parameter x = " + x
    }

    start() {
        var instance = new ConstructorWithParams(5)
        instance.doStuff(2)
        instance.doStuff(4)
    }

    doStuff(int y) {
        print "doing stuff on ConstructorWithParams instance" + " y = " + y
    }
}